    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (CoreWebAPI)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/CoreWebAPI/bin/Debug/netcoreapp3.1/CoreWebAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/CoreWebAPI",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Launch (测试控制台程序)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ConsoleApp/bin/Debug/netcoreapp3.1/ConsoleApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ConsoleApp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}